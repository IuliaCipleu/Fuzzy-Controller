        "VeryLow"
      ]
    },
    {
      "if": [
        [
          "annual_mileage",
          "High"
        ],
        [
          "prior_claims",
          "Many"
        ]
      ],
      "then": [
        "premium",
        "VeryHigh"
      ]
    }
  ]
