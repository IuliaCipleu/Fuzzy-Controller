        "Senior": {
          "kind": "trapmf",
          "params": [
            60,
            70,
            85,
            85
          ]
        }
      }
    },
    "prior_claims": {
      "min": 0,
      "max": 5,
      "step": 1,
      "mfs": {
        "None": {
          "kind": "trapmf",
          "params": [
            0,
            0,
            0,
