  "outputs": {
    "premium": {
      "min": 0,
      "max": 2000,
      "step": 1,
      "mfs": {
        "VeryLow": {
          "kind": "trapmf",
          "params": [
            0,
            0,
            300,
            500
          ]
        },
        "Low": {
          "kind": "trimf",
          "params": [
            300,
            600,
            900
          ]
        },
        "Medium": {
          "kind": "trimf",
          "params": [
            800,
            1100,
            1400
          ]
        },
        "High": {
          "kind": "trimf",
          "params": [
            1300,
            1600,
            1800
          ]
