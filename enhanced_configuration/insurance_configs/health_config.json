        }
      }
    },
    "pre_existing_conditions": {
      "min": 0,
      "max": 5,
      "step": 1,
      "mfs": {
        "None": {
          "kind": "trapmf",
          "params": [
            0,
            0,
            0,
            1
          ]
        },
        "Few": {
          "kind": "trimf",
          "params": [
